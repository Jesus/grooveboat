{
  "name": "grooveboat",
  "version": "1.0.0",
  "description": "A decentralized turntable.fm clone",
  "main": "src/js/electron.js",
  "author": "Steve Gattuso <steve@stevegattuso.me>",
  "license": "MIT",
  "alias": {
    "components": "./src/js/components",
    "pages": "./src/js/pages",
    "services": "./src/js/services",
    "db": "./src/js/db.js",
    "utils": "./src/js/utils"
