  "dependencies": {
    "axios": "^0.19.0",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.6",
    "go-ipfs-dep": "^0.4.21",
    "immutable": "^4.0.0-rc.12",
    "ipfsd-ctl": "^0.42.4",
    "jsmediatags": "^3.8.1",
    "jsonwebtoken": "^8.5.1",
    "pouchdb-browser": "^7.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-dropzone": "^10.1.5",
    "react-redux": "^7.1.0-rc.1",
    "react-router-dom": "^5.0.1",
    "readable-stream": "^3.4.0",
